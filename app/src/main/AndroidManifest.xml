<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.flipkartclone">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".UserActivity"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".accActivity"></activity>
        <activity android:name=".addressActivity"></activity>
        <activity android:name=".cartActivity"></activity>
        <activity android:name=".langActivity"></activity>
        <activity android:name=".notifActivity"></activity>
        <activity android:name=".orderActivity"></activity>
        <activity android:name=".reviewActivity"></activity>
    </application>

</manifest>